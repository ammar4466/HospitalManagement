@using Infrastructure.Model;
@model List<PatientInfo>
<div class="panel panel-hash">
    <div class="panel-heading"><h4><b>Recent Payments</b></h4></div>
    <div class="panel-body scroll panel-height">
        <table class="CustomBlueTable">
            <thead>
                <tr>
                    <th>Id</th>
                    <th>Patient</th>
                    <th>Discount(%)</th>
                    <th>Tax(%)</th>
                    <th>Sub Total</th>
                    <th>Grand Total</th>
                    <th>CreatedDate</th>
                    <th></th>
                </tr>
            </thead>
            <tbody>
                <tr>
                    @foreach (var item in Model)
                    {
                        int i = 0;
                        i++;
                        <td>@item.PatientId</td>
                        <td>@item.FullName</td>
                        <td></td>
                        <td></td>
                        <td></td>
                        <td></td>
                        <td></td>
                        <td></td>
                        <td></td>
                        <td></td>
                        <td></td>
                    }

                </tr>
            </tbody>
        </table>

        <br><br>
    </div>
</div>